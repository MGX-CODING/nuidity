{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "cli": {
    "analytics": false,
    "cache": {
      "enabled": true
    },
    "packageManager": "npm",
    "warnings": {
      "versionMismatch": false
    }
  },
  "schematics": {
    "@schematics/angular:component": {
      "changeDetection": "OnPush",
      "displayBlock": true,
      "standalone": true,
      "style": "scss"
    },
    "@schematics/angular:directive": {
      "standalone": true
    },
    "@schematics/angular:guard": {
      "functional": true
    },
    "@schematics/angular:interceptor": {
      "functional": true
    },
    "@schematics/angular:library": {
      "standalone": true
    },
    "@schematics/angular:pipe": {
      "standalone": true
    },
    "@schematics/angular:resolver": {
      "functional": true
    }
  },
  "projects": {
    "nuidity": {
      "projectType": "library",
      "root": "projects/nuidity",
      "sourceRoot": "projects/nuidity",
      "prefix": "nui",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/nuidity/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/nuidity/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/nuidity/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/nuidity/tsconfig.spec.json",
            "polyfills": ["zone.js", "zone.js/testing"],
            "karmaConfig": "projects/nuidity/karma.conf.js",
            "watch": false
          }
        }
      },
      "schematics": {
        "@schematics/angular:module": {
          "path": "projects/nuidity"
        },
        "@schematics/angular:component": {
          "path": "projects/nuidity",
          "inlineStyle": true,
          "standalone": false
        },
        "@schematics/angular:directive": {
          "path": "projects/nuidity",
          "standalone": false
        },
        "@schematics/angular:guard": {
          "path": "projects/nuidity",
          "functional": true
        },
        "@schematics/angular:interceptor": {
          "path": "projects/nuidity",
          "functional": true
        },
        "@schematics/angular:pipe": {
          "path": "projects/nuidity",
          "standalone": false
        },
        "@schematics/angular:resolver": {
          "path": "projects/nuidity",
          "functional": true
        },
        "@schematics/angular:service": {
          "path": "projects/nuidity"
        }
      }
    },
    "playground": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "skipTests": true,
          "standalone": false
        },
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true,
          "standalone": false
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:interceptor": {
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true,
          "standalone": false
        },
        "@schematics/angular:resolver": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "root": "projects/playground",
      "sourceRoot": "projects/playground/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/playground",
            "index": "projects/playground/src/index.html",
            "browser": "projects/playground/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/playground/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/playground/src/favicon.ico",
              "projects/playground/src/assets"
            ],
            "styles": [
              "projects/playground/src/styles.scss"
            ],
            "scripts": [],
            "server": "projects/playground/src/main.server.ts",
            "prerender": true,
            "ssr": {
              "entry": "projects/playground/server.ts"
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "playground:build:production"
            },
            "development": {
              "buildTarget": "playground:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "playground:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/playground/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/playground/src/favicon.ico",
              "projects/playground/src/assets"
            ],
            "styles": [
              "projects/playground/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "playground-standalone": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "skipTests": true
        },
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:interceptor": {
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:resolver": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "root": "projects/playground-standalone",
      "sourceRoot": "projects/playground-standalone/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/playground-standalone",
            "index": "projects/playground-standalone/src/index.html",
            "browser": "projects/playground-standalone/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/playground-standalone/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/playground-standalone/src/favicon.ico",
              "projects/playground-standalone/src/assets"
            ],
            "styles": [
              "projects/playground-standalone/src/styles.scss"
            ],
            "scripts": [],
            "server": "projects/playground-standalone/src/main.server.ts",
            "prerender": true,
            "ssr": {
              "entry": "projects/playground-standalone/server.ts"
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "playground-standalone:build:production"
            },
            "development": {
              "buildTarget": "playground-standalone:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "playground-standalone:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/playground-standalone/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/playground-standalone/src/favicon.ico",
              "projects/playground-standalone/src/assets"
            ],
            "styles": [
              "projects/playground-standalone/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  }
}
